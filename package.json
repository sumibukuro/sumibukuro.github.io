{
    "name": "sumibukuro",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "start": "open -a 'Google Chrome' http://localhost:8080/ && npx webpack-cli serve --mode development",
        "build": "npx webpack-cli build --mode production",
        "init_app": "cross-env NODE_ENV=app npm run build && mkdir -p platforms && cordova platform add android@11.0.0 electron && open ./platforms && cordova run electron",
        "build_app_debug": "cross-env NODE_ENV=app npm run build && npm run gen_app_icon && open ./platforms && cordova run android",
        "build_app_release": "cross-env NODE_ENV=app npm run build && npm run gen_app_icon && open ./platforms && dotenv -- bash -c 'cordova build android --release -- --keystore=.keystore --storePassword=$release_password --alias=apprelease --password=$release_password --packageType=bundle'",
        "gen_app_icon": "app-icon generate --adaptive-icons --platforms=android --icon=src/static/assets/icon.png --background-icon=src/static/assets/icon.png --foreground-icon=src/static/assets/icon.png",
        "gen_keystore": "keytool -genkey -v -keystore .keystore -alias apprelease -keyalg RSA -validity 10000",
        "lint": "eslint --fix '**/*.{js,jsx,ts,tsx}' && stylelint --fix '**/*.{css,scss,sass}' && prettier --write '**/*.{html,js,jsx,ts,tsx,css,scss,sass}'",
        "test": "jest",
        "storybook": "open -a 'Google Chrome' http://localhost:6006/ && start-storybook -p 6006 --no-open",
        "build-storybook": "build-storybook",
        "prepare": "husky install"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "cordova": {
        "platforms": [
            "electron",
            "android"
        ],
        "plugins": {}
    },
